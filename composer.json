{
    "name": "totem3/openapi-psr7-validator-for-70",
    "description": "fork of league/openapi-psr7-validator, in order to use from PHP7.0 temporarily",
    "keywords": [
        "OpenAPI",
        "psr7",
        "http",
        "validation"
    ],
    "homepage": "https://github.com/totem3/openapi-psr7-validator",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "League\\OpenAPIValidation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\OpenAPIValidation\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0",
        "ext-json": "*",
        "totem3/php-openapi-for70": "^1.3",
        "dflydev/fig-cookies": "^2.0",
        "psr/cache": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "respect/validation": "^1.1.3",
        "riverline/multipart-parser": "^2.0.3",
        "webmozart/assert": "^1.4"
    },
    "require-dev": {
        "cache/array-adapter": "^1.0",
        "doctrine/coding-standard": "^6.0",
        "guzzlehttp/psr7": "^1.5",
        "phpunit/phpunit": "^7|^8"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
